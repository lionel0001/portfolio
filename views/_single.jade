doctype html
html

   head
      meta(charset='utf8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
      meta(name='description', content='description of your site')
      meta(name='author', content= "author of the site")
      //- uncomment if your site is responsive!
      meta(name="viewport" content="width=device-width, initial-scale=1")
      title= _path

      link(href='/css/vendor/flexslider.css', rel='stylesheet')
      link(href='/css/vendor/slicknav.css', rel='stylesheet')

      != css('/')

         //- Picturefill
      script
         !='document.createElement( "picture" )'

      script(src='/js/vendor/jquery.picturefill.min.js' async) 

   body

      #bluestrip

      #container

         include _topnav

         #metaNav
            left

               span.vertSpace75
               h3
                  a(href='../') Back to the overview

         #projectDescript
            left
               h1= post.title
               h3
                  span.projectSubtitle
                     =post.subtitle
               h3
                  span.projectYear

                     if post.moreinfo
                        =post.year + " â€” "+ post.moreinfo
                     else
                        =post.year

               mixin picturefill_3 (img, mediaWidth, alt)
                  picture
                     !='<source srcset="' + img + '.png 1x, ' + img + '@2x.png 2x" media="(min-width:'+ mediaWidth +'px)">'
                     !='<img src="' + img + '@0,5x.png" srcset="' + img + ' 1x, ' + img + '.png 2x" alt="'+ alt +'">'

               div
                  +picturefill_3 ('/img/helen/top', '768', 'yahooo my name is indra')

